    {
      "cell_type": "code",
      "source": [
        "import torchvision.transforms as transforms\n",
        "dataset=MNIST(root='\\data', transform=transforms.ToTensor())\n",
        "from torch.utils.data import DataLoader \n",
        "from torch.utils.data import random_split"
      ],
      "metadata": {
        "id": "GxpvrElsuIBV"
      },
      "execution_count": 9,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "train_ds,val_ds=random_split(dataset,[50000,10000])\n",
        "batch_size=128\n",
        "train_loader=DataLoader(train_ds,batch_size=batch_size,shuffle=True)\n",
        "val_loader=DataLoader(val_ds,batch_size=batch_size)"
      ],
      "metadata": {
        "id": "12oZwMXPuvID"
      },
      "execution_count": 10,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import torch.nn as nn\n",
        "import torch.nn.functional as F\n",
        "input_size=28*28\n",
        "num_classes=10\n",
        "def loss_fn(out,label):\n",
        "  loss=F.cross_entropy(out,label)\n",
        "  return loss\n",
        "def accuracy(out,labels):\n",
        "  _,pred=torch.max(out,dim=1)\n",
        "  return torch.tensor(torch.sum(pred==labels).item()/len(pred))\n",
        "def evaluate(model,val_loader):\n",
        "  output=[model.validation_step(batch) for batch in val_loader ]\n",
        "  return model.validation_epoch_end(output)\n",
        "\n",
        "class MnistModel(nn.Module):\n",
        "  def __init__(self):\n",
        "    super().__init__()\n",
        "    self.linear= nn.Linear(input_size,num_classes)\n",
        "  def forward(self,x):\n",
        "    x=x.reshape(-1,784)\n",
        "    out=self.linear(x)\n",
        "    return out\n",
        "  def training_step(self,batch):\n",
        "    images,labels=batch\n",
        "    out=self(images)\n",
        "    loss=loss_fn(out,labels)\n",
        "    return loss\n",
        "  def validation_step(self,batch):\n",
        "    images,labels=batch\n",
        "    out=self(images)\n",
        "    loss=loss_fn(out,labels)\n",
        "    acc= accuracy(out,labels)\n",
        "    return {'val_loss': loss, 'val_acc': acc}\n",
        "  def validation_epoch_end(self,output):\n",
        "    batch_losses=[x['val_loss'] for x in output]\n",
        "    epoch_loss=torch.stack(batch_losses).mean()\n",
        "    batch_accs=[x['val_acc'] for x in output]\n",
        "    epoch_acc=torch.stack(batch_accs).mean()\n",
        "    return {'val_loss': epoch_loss.item(), 'val_acc':epoch_acc.item()}\n",
        "  def epochs_end(self,epoch,result):\n",
        "    print(f\"Epoch:{epoch}, val_loss:{result['val_loss']:.4f},val_acc:{result['val_acc']:.4f}\")\n",
        "model=MnistModel()\n",
        "model.linear"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "JRQs8DcTAX3k",
        "outputId": "12ca8a43-f2a7-47d9-ff42-7dc0e01c3a1d"
      },
      "execution_count": 11,
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/plain": [
              "Linear(in_features=784, out_features=10, bias=True)"
            ]
          },
          "metadata": {},
          "execution_count": 11
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "def fit(epochs,model,lr,val_loader,train_loader):\n",
        "  history=[]\n",
        "  optimizer=torch.optim.SGD(model.parameters(),lr=lr)\n",
        "  #training phase\n",
        "  for epoch in range(epochs):\n",
        "    for batch in train_loader:\n",
        "    #generate prediction\n",
        "      loss=model.training_step(batch)\n",
        "      loss.backward()\n",
        "      optimizer.step()\n",
        "      optimizer.zero_grad()\n",
        "      #validation phase\n",
        "    result=evaluate(model,val_loader)\n",
        "    model.epochs_end(epoch,result)\n",
        "    history.append(result)\n",
        "  return history"
      ],
      "metadata": {
        "id": "YdFwbOVgEs95"
      },
      "execution_count": 12,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "history1=fit(5,model,0.01,val_loader,train_loader)"
