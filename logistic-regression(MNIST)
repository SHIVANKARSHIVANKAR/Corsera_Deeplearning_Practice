    {
      "cell_type": "markdown",
      "source": [
        "Logistic Regresstion"
      ],
      "metadata": {
        "id": "Vr5ZQbIyWB-Q"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "from torch.autograd.grad_mode import no_grad\n",
        "import torch\n",
        "import torch.nn\n",
        "import numpy as np\n",
        "from sklearn import datasets\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "from sklearn.model_selection import train_test_split\n",
        "#prepare data\n",
        "bc=datasets.load_breast_cancer()\n",
        "X,y=bc.data,bc.target\n",
        "n_samples,n_features=X.shape\n",
        "X_train,X_test,y_train,y_test=train_test_split(X,y,test_size=0.2,random_state=1234)\n",
        "sc=StandardScaler()\n",
        "X_train=sc.fit_transform(X_train)\n",
        "X_test=sc.fit_transform(X_test)\n",
        "X_train=torch.from_numpy(X_train.astype(np.float32))\n",
        "X_test=torch.from_numpy(X_test.astype(np.float32))\n",
        "y_train=torch.from_numpy(y_train.astype(np.float32))\n",
        "y_test=torch.from_numpy(y_test.astype(np.float32))\n",
        "y_train=y_train.view(y_train.shape[0],1)\n",
        "y_test=y_test.view(y_test.shape[0],1)\n",
        "#model \n",
        "class LinearModel(nn.Module):\n",
        "    def __init__(self, n_input_features):\n",
        "        super(LinearModel, self).__init__()\n",
        "        self.linear = nn.Linear(n_input_features, 1)\n",
        "\n",
        "    def forward(self, x):\n",
        "        y_pred = torch.sigmoid(self.linear(x))\n",
        "        return y_pred\n",
        "\n",
        "model = LinearModel(n_features)\n",
        "#loss and optimizer\n",
        "lr=0.01\n",
        "n_epochs=100\n",
        "criterion=nn.BCELoss()\n",
        "optimizer=torch.optim.SGD(model.parameters(),lr=lr)\n",
        "# loop \n",
        "for epochs in range(n_epochs):\n",
        "  y_predicted=model(X_train)#forward pass\n",
        "  loss=criterion(y_predicted,y_train)#loss\n",
        "  #backward pass and update\n",
        "  loss.backward()\n",
        "  optimizer.step()\n",
        "  #zero grad before re-trainning\n",
        "  optimizer.zero_grad()\n",
        "  if epochs%10==0:\n",
        "    print(f'epoch: {epochs} loss={loss.item():.6f}')\n",
        "\n",
        "with torch.no_grad():\n",
        "  y_predicted=model(X_test)\n",
        "  y_predicted_ro=y_predicted.round()\n",
        "  acc=y_predicted_ro.eq(y_test).sum()/float(y_test.shape[0])\n",
        "  print(f'accuracy: {acc.item():.4f}')"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "grvB-U2bbR3f",
        "outputId": "d7d13be6-594b-405f-ef7e-4c4ea53b28c2"
